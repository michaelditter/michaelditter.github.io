{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/subscribe", "dest": "/api/subscribe.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "BUTTONDOWN_API_KEY": "@buttondown_api_key"
  }
} 